{
    "manifest_version":2,
    "theme":{
        "images":{
            "theme_frame":"images\/theme_frame.png",
            "theme_toolbar":"images\/theme_toolbar.png",
            "theme_tab_background":"images\/theme_tab_background.png",
            "theme_ntp_background":"images\/theme_background.gif"
        },
        "colors":{
            "frame":[26,26,26],
            "toolbar":[26,26,26],
            "tab_text":[255,255,255],
            "tab_background_text":[255,255,255],
            "bookmark_text":[255,255,255],
            "ntp_background":[26,26,26],
            "ntp_text":[255,255,255],
            "button_background":[0,0,0,0]
        },
        "tints":{
            "buttons":[0,0,0.5]
        },
        "properties":{
            "ntp_background_alignment":"bottom",
            "ntp_background_repeat":"no-repeat"
        }
    },
    "name":"Matrix Theme",
    "version":"1",
    "description":"A theme extension for Chrome browser that creates a dark style and animated Matrix background for the browser"
}
